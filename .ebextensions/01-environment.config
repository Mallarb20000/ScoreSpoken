option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "score.settings"
    DEBUG: "False"
    ALLOWED_HOSTS: "*"
    PYTHONUNBUFFERED: "1"

  aws:elasticbeanstalk:environment:proxy:
    ProxyServer: nginx

  aws:elasticbeanstalk:healthreporting:system:
    SystemType: enhanced